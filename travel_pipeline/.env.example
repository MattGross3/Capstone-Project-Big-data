# MongoDB connection string pointing at the mongos router
MONGODB_URI=mongodb://root:example@localhost:27017/?directConnection=true
# Logical database name for all collections
MONGODB_DATABASE=travel_ops
# Optional overrides for collection names
RAW_COLLECTION=flights_raw
CLEAN_COLLECTION=flights_clean
AGG_CARRIER_COLLECTION=agg_carrier_month
AGG_ORIGIN_COLLECTION=agg_origin_cancel
AGG_ROUTE_COLLECTION=agg_route_delay
METADATA_COLLECTION=metadata
# CSV locations (copy JAN/FEB datasets into the data folder or update the path)
JAN_FILE=data/JAN_DATA.csv
FEB_FILE=data/FEB_DATA.csv
# Batch tuning knobs
CHUNK_SIZE=100000
BATCH_SIZE=50000
